		<Parameter name="Exposure" float="0.9300001" />
		<Parameter name="DrawOrder" int="1" />
		<Parameter name="WhiteCutoff" float="0.04000001" />
		<Parameter name="Luminance" float="0.4399998" />
	  <Parameter name="DownSamples" int="1" />
	  <Parameter name="AdvancedGlow" bool="false" />
	</PostEffect>
